    "@biomejs/biome": "2.2.2",
    "@repo/tsconfig": "workspace:*",
    "@types/node": "^24.3.1",
    "dotenv-cli": "^10.0.0",
    "turbo": "^2.5.6",
    "typescript": "5.9.2"
  },
